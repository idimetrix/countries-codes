{
  "name": "countries-codes",
  "version": "1.1.0",
  "description": "Comprehensive data on countries, languages, and continents.",
  "license": "MIT",
  "author": {
    "name": "Dmitrii Selikhov",
    "email": "dmitrii.selikhov@gmail.com",
    "url": "https://www.linkedin.com/in/dimetrix"
  },
  "contributors": [
    {
      "name": "Dmitrii Selikhov",
      "email": "dmitrii.selikhov@gmail.com",
      "url": "https://www.linkedin.com/in/dimetrix"
    }
  ],
  "email": "dmitrii.selikhov@gmail.com",
  "scripts": {
    "build": "tsup ./src",
    "watch": "tsup ./src --watch",
    "run": "node dist/index.js",
    "dev": "pnpm run watch & nodemon dist/index.js",
    "prettier": "prettier --write src/** package.json README.md",
    "format": "pnpm run prettier && pnpm type",
    "bootstrap": "pnpm install && pnpm format",
    "type": "tsc --noEmit --incremental",
    "tsc:clean": "tsc --build --clean",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docs": "typedoc --entryPoints ./src/index.ts --out docs --plugin typedoc-plugin-markdown"
  },
  "homepage": "https://github.com/idimetrix/countries-codes#readme",
  "url": "https://github.com/idimetrix/countries-codes/issue",
  "bugs": {
    "url": "https://github.com/idimetrix/countries-codes/issues"
  },
  "keywords": [
    "countries",
    "country-codes",
    "iso-3166",
    "iso-639",
    "languages",
    "continents",
    "currency",
    "phone-codes",
    "emoji-flags",
    "typescript",
    "country-data",
    "localization",
    "i18n"
  ],
  "files": [
    "dist",
    "docs",
    "package.json",
    "README.md"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "dependencies": {
    "react": "19.1.1",
    "typedoc": "0.28.13",
    "typedoc-plugin-markdown": "4.9.0"
  },
  "devDependencies": {
    "@types/jest": "30.0.0",
    "@types/node": "24.5.2",
    "husky": "9.1.7",
    "jest": "30.1.3",
    "lint-staged": "16.2.0",
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "ts-jest": "29.4.4",
    "ts-node": "10.9.2",
    "tsup": "8.5.0",
    "typescript": "5.9.2"
  },
  "lint-staged": {
    "*.{ts,js,json,md}": [
      "prettier --write"
    ],
    "*.ts": [
      "tsc --noEmit"
    ]
  }
}
